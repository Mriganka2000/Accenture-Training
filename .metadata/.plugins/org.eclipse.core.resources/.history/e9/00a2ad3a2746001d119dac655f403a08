package com.polymorphism;

class Parent {
	
	public void show() {
		System.out.println("From parent");
	}
	
}

class Child extends Parent {
	
	public void show() {
		super.show();
		System.out.println("From child");
	}
	
}

public class MethodOverriding {

	public static void main(String[] args) {
		Parent p = new Child();
		
		p.show();
	}

}
