package com.test;

public class Main {
	
	public static void main(String[] args) {
		 ParticipateA a = new Hybrid();
		 ParticipateB b = new Hybrid();
		 
		 a.setParticipateA("Accenture", 1, "Java", "Apereal");
		 b.setParticipateB("Accenture", 2, "SAP", "Medica");
		 
		 Hybrid h1 = (Hybrid)a;
		 Hybrid h2 = (Hybrid)b;
		 
		 System.out.println(h1.companyName);
		 System.out.println(h1.eid);
		 System.out.println(h1.stream);
		 System.out.println(h1.project);
		 
		 System.out.println(h2.companyName);
		 System.out.println(h2.eid);
		 System.out.println(h2.stream);
		 System.out.println(h2.project);
	}
	
}
