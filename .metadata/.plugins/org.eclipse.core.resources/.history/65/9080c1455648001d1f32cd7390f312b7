package com.algorithm;

public class ExceptionTest {

	public static void main(String[] args) {
		try {
			int[] arr = new int[10];
			arr[10] = 100;
			
			String num = "123ad";
			System.out.println(Integer.parseInt(num));
		}
		catch (Exception e) {
			System.out.println(e.getMessage());
		}
	}
	
}
